FUNCTION (src_rec ORDB011M) getORDB011M()
    DEFINE order_no_tmp, order_item_tmp STRING
    
    SET CONNECTION "ORD"
    SELECT * INTO src_rec.* FROM ORDB011M
        WHERE ORDER_NO = src_rec.ORDER_NO
            AND ORDER_ITEM = src_rec.ORDER_ITEM

    IF SQLCA.sqlcode == 100 THEN
        LET order_no_tmp = src_rec.ORDER_NO
        LET order_item_tmp = src_rec.ORDER_ITEM
        INITIALIZE src_rec.* TO NULL
        LET src_rec.ORDER_NO = order_no_tmp
        LET src_rec.ORDER_ITEM = order_item_tmp
    END IF
END FUNCTION